public class NomeEventoTest extends TestCase {

private NomeEvento validanomeevento = new NomeEvento();

publicvoid testIsValidaNomeEvento(){

assertFalse(“retorna FALSE – NomeEvento inválido”, validanomeenvento.isValidaNomeEvento(“128641011”));

assertFalse(“retorna FALSE – NomeEvento inválido”, validanomeevento.isValidaNomeEvento(null));

assertFalse(“retorna FALSE – NomeEvento inválido”, validanomeevento.isValidaNomeEvento(“”));

assertTrue(“retorna TRUE – NomeEvento válido”, validanomeevento.isValidaNomeEvento(“Festival de Musica”));

}
